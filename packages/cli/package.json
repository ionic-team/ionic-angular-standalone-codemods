{
  "name": "@ionic/angular-standalone-codemods",
  "version": "0.0.1",
  "dependencies": {
    "@angular-eslint/template-parser": "^16.1.2",
    "@clack/core": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "picocolors": "^1.0.0",
    "ts-dedent": "^2.2.0",
    "ts-morph": "^19.0.0"
  },
  "bin": "./dist/index.js",
  "scripts": {
    "start": "jiti ./src/index.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "watch": "pnpm run build --watch",
    "dev": "pnpm run build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitest/coverage-v8": "^0.34.5",
    "eslint": "^8.0.0",
    "jiti": "^1.20.0",
    "tsup": "^7.2.0",
    "typescript": "^4.3.5",
    "vitest": "^0.34.5"
  },
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts"
}
